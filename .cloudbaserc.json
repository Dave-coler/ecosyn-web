{
  "version": "2.0",
  "envId": "你的环境ID",
  "framework": {
    "name": "ecosyn-web",
    "plugins": {
      "website": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "outputPath": "dist",
          "cloudPath": "/",
          "ignore": ["node_modules/**", ".git/**"],
          "buildCommand": "npm install && npm run build"
        }
      }
    }
  }
}
